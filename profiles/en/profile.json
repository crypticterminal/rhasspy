{
    "language": "en",

    "wake": {
        "system": "pocketsphinx",
        "pocketsphinx": {
            "keyphrase": "okay rhasspy",
            "threshold": 1e-40
        }
    },

    "training": {
        "sentence_casing": "lower",
        "regex": {
            "replace": [
                { "'s": "s" },
                { "[^\\w ]+": " " }
            ]
        }
    }
}
