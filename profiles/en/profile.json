{
    "home_assistant": {
        "api_password": "",
        "url": "http://hassio/homeassistant/"
    },
    "intent": {
        "remote": {
            "url": "http://pumpkin.lan:12101/api/text-to-intent"
        },
        "system": "fuzzywuzzy"
    },
    "language": "en",
    "speech_to_text": {
        "remote": {
            "url": "http://pumpkin.lan:12101/api/speech-to-text"
        },
        "system": "pocketsphinx"
    },
    "training": {
        "regex": {
            "replace": [
                {
                    "'s": "s"
                },
                {
                    "[^\\w ]+": " "
                }
            ]
        },
        "sentence_casing": "lower"
    },
    "wake": {
        "pocketsphinx": {
            "keyphrase": "okay rhasspy",
            "threshold": 1e-40
        },
        "system": "pocketsphinx"
    }
}